/* /* ko/search.json  v6.4.1_202510180200 */ */

{%- assign posts_lang = site.posts | where_exp:'p','p.lang == "ko"' -%}
[
{%- for p in posts_lang -%}
  {
    "title": {{ p.title | jsonify }},
    "url": {{ p.url | relative_url | jsonify }},
    "date": "{{ p.date | date: "%Y-%m-%d" }}",
    "tags": {{ p.tags | jsonify }},
    "content": {{ p.content | strip_html | normalize_whitespace | jsonify }}
  }{%- unless forloop.last -%},{%- endunless -%}
{%- endfor -%}
]

