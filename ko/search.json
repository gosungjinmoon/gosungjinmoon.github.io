/* ko/search.json  v202510221600 */
{% assign lang = 'ko' %}
[
{% assign first = true %}
{% for post in site.posts | where: "lang", lang %}
  {% unless first %},{% endunless %}
  {% assign first = false %}
  {
    "title": "{'{'} post.title | escape {'}'}",
    "url": "{'{'} post.url | relative_url {'}'}",
    "date": "{'{'} post.date | date: "%Y-%m-%d" {'}'}",
    "tags": [{'{'} post.tags | join: '","' | prepend: '"' | append: '"' | strip_newlines {'}'}],
    "lang": "{lang}",
    "excerpt": "{'{'} post.excerpt | strip_newlines | strip | escape {'}'}"
  }
{% endfor %}
]
